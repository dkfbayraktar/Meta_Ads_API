{
  "openapi": "3.1.0",
  "info": {
    "title": "Meta Ads Insights API",
    "description": "Günlük ve saatlik Meta reklam verilerini çeker.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://dkfbayraktar.github.io/Meta_Ads_API"
    }
  ],
  "paths": {
    "/daily-insights": {
      "get": {
        "summary": "Günlük performans verilerini getirir",
        "operationId": "getDailyInsights",
        "responses": {
          "200": {
            "description": "JSON formatında günlük performans verisi",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/hourly-insights": {
      "get": {
        "summary": "Saatlik performans verilerini getirir",
        "operationId": "getHourlyInsights",
        "responses": {
          "200": {
            "description": "JSON formatında saatlik performans verisi",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}

